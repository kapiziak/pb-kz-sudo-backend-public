{
    "name": "pb-kz-sudo-backend",
    "version": "1.0.0",
    "description": "",
    "main": "dist/src/index.js",
    "scripts": {
        "predev": "npm run api",
        "prebuild": "npm run api",
        "api": "ts-node swagger/swagger.ts",
        "build": "rm -rf ./dist && mkdir ./dist && npx tsc",
        "start": "node dist/src/index.js",
        "dev": "rm -rf ./dist && mkdir ./dist && concurrently \"npx tsc --watch\" \"nodemon -q dist/src/index.js\"",
        "docker-test-db:up": "docker-compose up testDb -d",
        "docker-test-db:down": "docker-compose down testDb",
        "docker-test-db:reset": "dotenv -e .env.test -- yarn prisma migrate reset --force",
        "docker-test-db:migrate": "dotenv -e .env.test -- yarn prisma migrate deploy",
        "docker-test-db:prisma-studio": "dotenv -e .env.test -- yarn prisma studio",
        "test": "yarn run docker-test-db:reset && yarn run docker-test-db:migrate && dotenv -e .env.test -- jest -i"
    },
    "author": "Kacper Zakrzewski",
    "license": "ISC",
