        "@types/nodemailer": "^6.4.14",
        "@types/passport-jwt": "^3.0.9",
        "bcrypt": "^5.1.0",
        "cookie": "^0.5.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.1",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.7",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "swagger-autogen": "^2.23.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^4.6.0",
        "ts-node": "^10.9.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.16",
        "@types/jest": "^29.5.6",
        "@types/multer": "^1.4.11",
